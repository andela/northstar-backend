{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "Make company global travel and accommodation easy and convenient",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build": "babel src -d dist --copy-files",
    "start:dev": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "start:staging": "NODE_ENV=staging node dist/index.js",
    "start": "NODE_ENV=production node dist/index.js",
    "docker:on": "docker-compose up -d",
    "docker:off": "docker-compose down",
    "docker:rebuild": "docker-compose up --rebuild",
    "docker:destroy": "docker-compose down -v",
    "lint": "eslint ./src --fix",
    "test": "npm run undo:migration && npm run db:migrate && nyc mocha -r esm --cache false  --exit",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "undo:migration": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "exclude": [
      "src/index.js"
    ]
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-subset": "^1.6.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "esm": "^3.2.25",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "pug": "^2.0.4",
    "request": "^2.87.0",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^5.5.0",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "coveralls": "^3.0.6",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
